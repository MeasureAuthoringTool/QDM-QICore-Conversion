            <definition context="Patient" id="846d538f-4693-4a4b-af51-38b4dc8b98d5"
                        name="Emergency Department Visit During Measurement Period With Order for Admission"
                        popDefinition="false" supplDataElement="false">
                <logic>( ["Encounter, Performed": "Emergency Department Visit"]
                    union ["Encounter, Performed": "Critical care, evaluation and management of the critically ill or
                    critically injured patient; first 30-74 minutes"] ) EDVisit
                    with ["Encounter, Order": "Hospital Admission or Observation"] OrderForAdmission
                    such that EDVisit.relevantPeriod during "Measurement Period"
                    and OrderForAdmission.authorDatetime during EDVisit.relevantPeriod
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="d83908e8-96ce-4274-a3b7-783638f94345"
                        name="Emergency Department Visit for Septic Shock" popDefinition="false"
                        supplDataElement="false">
                <logic>"Emergency Department Visit with Septic Shock Diagnosis"
                    union "Emergency Department Visit with Acute Hypotension and Sepsis or Infection Diagnoses"
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="1b2ab546-e4be-4667-b908-1c6ec7e8c4fb" name="Denominator"
                        popDefinition="false" supplDataElement="false">
                <logic>"Initial Population"</logic>
                <comment/>
            </definition>
            <definition context="Patient" id="32f32ad5-1b16-4993-a933-b589da4c8187" name="Patient Declined Sepsis Care"
                        popDefinition="false" supplDataElement="false">
                <logic>"Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    with ["Assessment, Performed": "Declined Sepsis Care"] DeclinedSepsisCare
                    such that DeclinedSepsisCare.authorDatetime during SepticShockEDVisit.relevantPeriod
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="0941b79e-d2b8-4239-873f-4226700ba01b"
                        name="Patient Receiving Comfort Measures" popDefinition="false" supplDataElement="false">
                <logic>( "Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    with ["Intervention, Order": "Comfort Measures"] OrderForComfortMeasures
                    such that OrderForComfortMeasures.authorDatetime during SepticShockEDVisit.relevantPeriod
                    )
                    union ( "Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    with ["Intervention, Performed": "Comfort Measures"] ReceivingComfortMeasures
                    such that ReceivingComfortMeasures.relevantPeriod overlaps after SepticShockEDVisit.relevantPeriod
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="11e89545-b1d7-4a2d-bc85-4f15739c7a6c"
                        name="Patient Left Before Treatment Completion" popDefinition="false" supplDataElement="false">
                <logic>"Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    where SepticShockEDVisit.dischargeDisposition in "Left Before Treatment Completion"
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="9ed40ae0-21b1-4bce-9321-550bb8e0eba4" name="Initial Population"
                        popDefinition="false" supplDataElement="false">
                <logic>( "Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    with ["Patient Characteristic Birthdate": "Birth date"] BirthDate
                    such that Global."CalendarAgeInYearsAt" ( BirthDate.birthDatetime, start of
                    SepticShockEDVisit.relevantPeriod ) &gt;= 18
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="921e72e0-e036-4c1f-a21f-63e78cb240a8"
                        name="Emergency Department Visit for Septic Shock with Other Secondary Diagnosis"
                        popDefinition="false" supplDataElement="false">
                <logic>"Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    where exists ( SepticShockEDVisit.diagnoses SepticShockEDVisitDiagnosis
                    where ( SepticShockEDVisitDiagnosis.code in "Cardiac Arrest"
                    or SepticShockEDVisitDiagnosis.code in "Second and Third Degree Burn"
                    or SepticShockEDVisitDiagnosis.code in "Seizure"
                    or SepticShockEDVisitDiagnosis.code in "Gastrointestinal Hemorrhage"
                    or SepticShockEDVisitDiagnosis.code in "Hemorrhagic Stroke"
                    or SepticShockEDVisitDiagnosis.code in "Ischemic Stroke"
                    or SepticShockEDVisitDiagnosis.code in "Myocardial Infarction"
                    or SepticShockEDVisitDiagnosis.code in "Toxicological Emergency"
                    or SepticShockEDVisitDiagnosis.code in "Trauma"
                    )
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="0a2de8f0-2713-420f-8331-54bac7e0109b"
                        name="Emergency Department Visit with Acute Hypotension and Sepsis or Infection Diagnoses"
                        popDefinition="false" supplDataElement="false">
                <logic>"Emergency Department Visit During Measurement Period With Order for Admission" EDEncounter
                    where exists ( EDEncounter.diagnoses EDEncounterDiagnosis
                    where EDEncounterDiagnosis.code in "Acute Hypotension"
                    and ( EDEncounterDiagnosis.code in "Infection"
                    or EDEncounterDiagnosis.code in "Sepsis"
                    )
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="4932674c-b900-4750-b56d-a20ba16acdd7"
                        name="Emergency Department Visit with Septic Shock Diagnosis" popDefinition="false"
                        supplDataElement="false">
                <logic>"Emergency Department Visit During Measurement Period With Order for Admission" EDEncounter
                    where exists ( EDEncounter.diagnoses EDEncounterDiagnosis
                    where EDEncounterDiagnosis.code in "Septic Shock"
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="4986bfcd-a634-4744-b08b-cb559c1f1e07" name="Numerator"
                        popDefinition="false" supplDataElement="false">
                <logic>"Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    with ["Medication, Order": "IV Antibiotics for Sepsis"] SepsisAntibioticsOrder
                    such that SepsisAntibioticsOrder.authorDatetime during SepticShockEDVisit.relevantPeriod
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="96255bb6-955a-416a-98d1-e8a98585fe45" name="Denominator Exclusions"
                        popDefinition="false" supplDataElement="false">
                <logic>"Patient Transferred from Acute Care or Inpatient Facility"
                    union "Patient Left Before Treatment Completion"
                    union "Patient Died During Emergency Department Visit for Septic Shock"
                    union "Patient Receiving Comfort Measures"
                    union "Patient Declined Sepsis Care"
                    union "Emergency Department Visit for Septic Shock with Other Secondary Diagnosis"
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="65b13916-7808-40cc-ad04-e2887f63419e"
                        name="Patient Transferred from Acute Care or Inpatient Facility" popDefinition="false"
                        supplDataElement="false">
                <logic>"Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    where SepticShockEDVisit.admissionSource in "Acute Care or Inpatient Facility"
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" id="ef3f1e17-aba4-481c-9bea-f5a1ea120e89"
                        name="Patient Died During Emergency Department Visit for Septic Shock" popDefinition="false"
                        supplDataElement="false">
                <logic>"Emergency Department Visit for Septic Shock" SepticShockEDVisit
                    where ( SepticShockEDVisit.dischargeDisposition ~ "Patient deceased during stay (discharge status =
                    dead) (finding)"
                    or SepticShockEDVisit.dischargeDisposition ~ "Expired (i.e. dead)"
                    )
                </logic>
                <comment/>
            </definition>
        </definitions>
